{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./CSV/en/countries.csv",
          "file_format": "csv",
          "tag": "countries CSV",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "excludeHeaderRow": true,
          "language": "en"
        },
        {
          "path": "./CSV/en/payments.csv",
          "file_format": "csv",
          "tag": "payments CSV",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "excludeHeaderRow": true,
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./CSV/<language>/<language>_countries.csv",
          "file_format": "csv",
          "tag": "countries CSV",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "columnDescription": {
            "KEY": 0,
            "<language>": 1
          },
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./CSV/<language>/<language>_payments.csv",
          "file_format": "csv",
          "tag": "payments CSV",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "columnDescription": {
            "KEY": 0,
            "<language>": 1
          },
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}